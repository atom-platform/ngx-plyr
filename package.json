{
  "name": "@atom-platform/ngx-plyr-app",
  "version": "15.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build --configuration production && cp dist/ngx-plyr-app/index.html dist/ngx-plyr-app/404.html",
    "build:lib": "ng build ngx-plyr --configuration production && cp README.md dist/ngx-plyr && cp LICENSE.md dist/ngx-plyr",
    "test:app": "ng test ngx-plyr-app",
    "test:app:ci": "ng test ngx-plyr-app --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:lib": "ng test ngx-plyr",
    "test:lib:ci": "ng test ngx-plyr --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npm-pack": "cd dist/ngx-plyr && npm pack",
    "package": "npm run build:lib && npm run npm-pack",
    "publish": "npm publish dist/ngx-plyr --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@types/hls.js": "^0.12.4",
    "core-js": "^2.6.9",
    "dashjs": "^4.7.4",
    "hls.js": "^1.5.8",
    "plyr": "^3.7.8",
    "rxjs": "~6.5.4",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/jasmine": "^3.3.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.2.2",
    "ts-node": "~7.0.0",
    "typescript": "^4.9.5"
  }
}
